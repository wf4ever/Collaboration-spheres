    function getAdjacentsNodesByType(idNode,type){
      if (typeof(graph.adjacency[idNode]) !== 'undefined'){
        var nodes = new Array();
        $.each(graph.adjacency[idNode], function(i,adj){
              console.log("id: " + i );
              $.each(adj, function(i,edge){
                 if (edge.data.type == type){
                    console.log("type: " + edge.data.type + " target: " + edge.target.data.name);
                    nodes.push(edge.target);
                  }          
              });
        });
        return nodes;
      }
      return null;
    }
    
